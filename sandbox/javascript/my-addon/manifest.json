{

  "manifest_version": 2,
  "name": "Download with Transmission",
  "version": "1.0",

  "description": "Download a link with Transmission.",

  "icons": {
    "64": "icons/icon-64.png",
    "32": "icons/icon-32.png",
    "16": "icons/icon-16.png"
  },

  "permissions": [
    "contextMenus",
    "http://*/",
    "https://*/"
  ],

  "background": {
    "scripts": ["lib/main.js"]
  },
  "options_ui": {
    "page": "views/options.html",
    "browser_style": true
  }

}
