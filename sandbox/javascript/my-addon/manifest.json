{

  "manifest_version": 2,
  "name": "Script Blocker",
  "version": "1.0",

  "description": "Download a link with Transmission.",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": [
      "chrome/main.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/main.js"]
    }
  ]

}
